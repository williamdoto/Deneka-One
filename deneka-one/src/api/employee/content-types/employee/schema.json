{
  "kind": "collectionType",
  "collectionName": "employees",
  "info": {
    "singularName": "employee",
    "pluralName": "employees",
    "displayName": "Employee",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "employee_email": {
      "type": "email",
      "required": true
    },
    "employee_name": {
      "type": "string",
      "required": true,
      "minLength": 2
    },
    "employee_join_date": {
      "type": "date",
      "required": true
    },
    "employee_birth_date": {
      "type": "date"
    },
    "employee_id": {
      "type": "uid",
      "required": true
    },
    "employee_phone": {
      "type": "integer",
      "required": true
    },
    "employee_address": {
      "type": "string"
    },
    "company": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::company.company"
    },
    "employee_password": {
      "type": "string",
      "private": true,
      "minLength": 8,
      "required": true
    }
  }
}
